# Inteli-Packs Environment Variables Example
# Copy this file to .env and fill in your actual API keys

# Required: At least one AI provider API key is needed
# Google Gemini API (Free tier available)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API (Paid service)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API (Paid service)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Groq API (Free tier available)
GROQ_API_KEY=your_groq_api_key_here

# Perplexity API (Free tier available)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# OpenRouter API (Paid service)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Azure OpenAI API (Enterprise)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_DEPLOYMENT_NAME=your_azure_deployment_name_here

# Cohere API (Paid service)
COHERE_API_KEY=your_cohere_api_key_here

# Hugging Face API (Free tier available)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Replicate API (Paid service)
REPLICATE_API_KEY=your_replicate_api_key_here

# Together AI API (Paid service)
TOGETHER_API_KEY=your_together_api_key_here

# Ollama (Local - no API key needed)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# LLaMA (Local - no API key needed)
LLAMA_MODEL_PATH=path_to_your_local_llama_model

# Optional: Model-specific configurations
GEMINI_MODEL=gemini-1.5-flash
OPENAI_MODEL=gpt-4
CLAUDE_MODEL=claude-3-sonnet-20240229
GROQ_MODEL=llama3-8b-8192
PERPLEXITY_MODEL=llama-3.1-8b-instant
OPENROUTER_MODEL=openai/gpt-4
COHERE_MODEL=command
HUGGINGFACE_MODEL=meta-llama/Llama-2-70b-chat-hf
REPLICATE_MODEL=meta/llama-2-70b-chat:02e509c789ffa4e38dc4c3c8c0b5c7c0b5c7c0b5c7c0b5c7c0b5c7c0b5c7c0b5
TOGETHER_MODEL=togethercomputer/llama-2-70b
